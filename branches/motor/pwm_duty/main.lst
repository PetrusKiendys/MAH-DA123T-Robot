 141:main.c        ****  * Description:
 142:main.c        ****  *    Delay execution by a specified number of milliseconds by using
 143:main.c        ****  *    timer #1. A polled implementation.
 144:main.c        ****  *
 145:main.c        ****  * Params:
 146:main.c        ****  *    [in] delayInMs - the number of milliseconds to delay.
 147:main.c        ****  *
 148:main.c        ****  ****************************************************************************/
 149:main.c        **** static void
ARM GAS  /cygdrive/c/DOCUME~1/deforce/LOKALA~1/Temp/ccmORfSV.s 			page 4


 150:main.c        **** delayMs(tU16 delayInMs)
 151:main.c        **** {
 152:main.c        ****   /*
 153:main.c        ****    * setup timer #1 for delay
 154:main.c        ****    */
 155:main.c        ****   TIMER1_TCR = 0x02;          //stop and reset timer
 156:main.c        ****   TIMER1_PR  = 0x00;          //set prescaler to zero
 157:main.c        ****   TIMER1_MR0 = delayInMs * ((CRYSTAL_FREQUENCY * PLL_FACTOR)/ (1000 * VPBDIV_FACTOR));
 158:main.c        ****   TIMER1_IR  = 0xff;          //reset all interrrupt flags
 159:main.c        ****   TIMER1_MCR = 0x04;          //stop timer on match
 160:main.c        ****   TIMER1_TCR = 0x01;          //start timer
 161:main.c        ****   
 162:main.c        ****   //wait until delay time has elapsed
 163:main.c        ****   while (TIMER1_TCR & 0x01)
 164:main.c        ****     ;
 165:main.c        **** }
